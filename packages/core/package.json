{
  "name": "node-ts-cache",
  "description": "Simple and extensible caching module supporting decorators",
  "version": "4.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "tdd": "mocha --require ts-node/register --watch --watch-files src, test/**/*.ts",
    "build": "tsc -p .",
    "clean": "git clean -fdx src test",
    "ts-dev": "tsc -p . -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/havsar/node-ts-cache.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "cache",
    "typescript",
    "ts",
    "caching",
    "memcache",
    "memory-cache",
    "redis-cache",
    "redis",
    "cache",
    "file-cache",
    "node-cache",
    "ts-cache"
  ],
  "author": "Himmet Avsar <avsar.himmet1@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/havsar/node-ts-cache/issues"
  },
  "homepage": "https://github.com/havsar/node-ts-cache#readme",
  "dependencies": {
    "@types/bluebird": "3.5.33",
    "@types/node": "14.14.10",
    "@types/redis": "^2.8.28",
    "bluebird": "3.7.2",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@types/mocha": "8.0.4",
    "@types/proxyquire": "1.3.28",
    "@types/sinon": "9.0.9",
    "mocha": "8.2.1",
    "proxyquire": "2.1.3",
    "sinon": "9.2.1",
    "ts-node": "9.0.0",
    "typescript": "4.1.2"
  }
}
